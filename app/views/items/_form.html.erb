<div class="container">
  <%= simple_form_for(item) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :age, input_html: { type: 'number', min: '1' } %>
    <%= f.input :house %>
    <%= f.input :image, as: :file %>
    <%= f.submit "Save Item!" %>
  <% end %>
</div>
